{
    "version": 2,
    "builds": [
    {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
            "includeFiles":[
                "prisma/schema.prisma",
                "node_modules/.prisma/client/"
            ]
        }
    }
    ],
    
    "routes": [
    {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
 }
]
}